{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Blog_Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Blog_Releasing_Date": {
      "type": "date"
    },
    "Blog_Content": {
      "type": "richtext"
    },
    "Blog_Image_Single": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "blog"
    },
    "slug": {
      "type": "string"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::member.member"
    }
  }
}
